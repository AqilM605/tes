{
  "name": "simpleapi",
  "version": "1.0.0",
  "description": "",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "(/__test__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/app/**/*.js"
    ]
  },
  "test": "jest --testEnvironment=node --runInBand ./tests",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "devstart": "nodemon server.js"
  },
  "author": "Aqil",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-env": "^7.14.7",
    "babel": "^6.23.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jest": "^27.0.5",
    "mongodb-memory-server": "^6.9.6",
    "mongoose": "^5.12.14"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "jest-mock-req-res": "^1.0.2",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  }
}
